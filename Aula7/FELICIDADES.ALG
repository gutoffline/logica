Algoritmo "FELICIDADES"
// Disciplina  :  [Linguagem e Lógica de Programação] 
// Professor   : Antonio Carlos Nicolodi 

Var
   contador : inteiro

Inicio
   contador := 7
   enquanto contador <= 20 faca
      escreval(contador)

      se contador = 15 entao
         escreval("Felicidades")
         interrompa
      fimse
      
      contador := contador + 1
   fimenquanto
   

Fimalgoritmo